---
import StaticPageWrapper from '@components/StaticPageWrapper.astro'
import Title from '@components/Title.astro'
import Subtitle from '@components/Subtitle.astro'
import SEO from '@components/SEO.astro'

const lang = 'en'
---

<SEO slot='seo' title={`The Yemen Listening Project`} description={'Where Yemenis talk, and the world listens.'} />

<StaticPageWrapper lang={lang}>
	<div slot='header'>
		<Title>About</Title>
		<Subtitle>Read more about the Yemen Listening Project.</Subtitle>
	</div>

	<div slot='body'>
		<p><a href='/en'>Go to English version</a></p>
		<p><a href='/ar'>Go to Arabic vesion</a></p>
	</div>
</StaticPageWrapper>
